{
    "env": {
      "node": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-console": "error",
      "no-underscore-dangle": "off",
      "consistent-return": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "no-param-reassign":"off"
    },
    "plugins": [
      "import",
      "node"
    ]
  }
  